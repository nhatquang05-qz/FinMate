{
  "expo": {
    "name": "FinMate",
    "slug": "FinMate",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ie307.finmate"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ie307.finmate"
    },
    "web": {},
    "plugins": [
      "expo-font",
      "expo-sqlite"
    ],
    "scheme": "finmate"
  }
}